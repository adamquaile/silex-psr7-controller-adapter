{
    "name": "voidcontext/silex-psr7-controller-adapter",
    "description": "PSR-7 controller adapter for Silex",
    "keywords": [ "Silex", "PSR-7", "psr7", "Controller", "Adapter", "HTTP", "HTTP message"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gabor Pihaj",
            "email": "gabor.pihaj@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "psr/http-message": "^1.0",
        "symfony/psr-http-message-bridge": "^0.2.0|^1.0",
        "zendframework/zend-diactoros": "^1.3",
        "silex/silex": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "codeclimate/php-test-reporter": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "SilexPsr7\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilexPsr7Test\\": "test"
        }
    }
}
